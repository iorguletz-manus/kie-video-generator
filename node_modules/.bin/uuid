#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/kie-video-generator/node_modules/uuid/dist/esm/bin/node_modules:/home/ubuntu/kie-video-generator/node_modules/uuid/dist/esm/node_modules:/home/ubuntu/kie-video-generator/node_modules/uuid/dist/node_modules:/home/ubuntu/kie-video-generator/node_modules/uuid/node_modules:/home/ubuntu/kie-video-generator/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/kie-video-generator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/kie-video-generator/node_modules/uuid/dist/esm/bin/node_modules:/home/ubuntu/kie-video-generator/node_modules/uuid/dist/esm/node_modules:/home/ubuntu/kie-video-generator/node_modules/uuid/dist/node_modules:/home/ubuntu/kie-video-generator/node_modules/uuid/node_modules:/home/ubuntu/kie-video-generator/node_modules:/home/ubuntu/node_modules:/home/node_modules:/node_modules:/home/ubuntu/kie-video-generator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uuid/dist/esm/bin/uuid" "$@"
else
  exec node  "$basedir/../uuid/dist/esm/bin/uuid" "$@"
fi
